{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "description": "visualisation of sleep pattern",
    "data": {
      "url": "/data/test1.json"
    },
"repeat": {
    "column": [
      "count_WAKE",
      "count_LIGHT_SLEEP",
      "count_DEEP_SLEEP",
      "count_REM_SLEEP"
    ]
  },
  "spec": {
    "mark": "bar",
    "encoding": {
      "y": {
        "timeUnit": "utcdate",
        "field": "sessionStartTime",
        "type": "ordinal"
      },
      "x":{
        "field": {"repeat":"column"} ,
        "type": "quantitative",
        "stack":"normalize"
        
      },
      "color": {
        "field": 
        {"repeat":"column"} ,
        "type": "nominal"
      }
    }
  }
}